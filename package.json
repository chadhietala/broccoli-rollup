    "fs-tree-diff": "^0.5.2",
    "heimdalljs": "^0.2.6",
    "heimdalljs-logger": "^0.1.10",
    "magic-string": "^0.25.1",
    "node-modules-path": "^1.0.1",
    "rollup": "^0.67.4",
    "symlink-or-copy": "^1.1.8",
    "walk-sync": "^0.3.1"
